FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 4.5e-05;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    cone
    {
        type            boundaryCorrectedFixedValue;
        value           uniform 0.0;
    }
    top_bot
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    syms
    {
        type            symmetry;
    }
}


// ************************************************************************* //
