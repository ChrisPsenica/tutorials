FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];


internalField   uniform 100;

boundaryField
{
    "(blade|spinner)"
    {
        type            omegaWallFunction;
        value           uniform 100;
        blended         true;
    }
    "(inlet|outlet|outer|inout)"
    {
        type            inletOutlet;
        inletValue      uniform 100;
        value           uniform 100;
    }
    "cyc.*"
    {
        type            cyclic;
    }
}


