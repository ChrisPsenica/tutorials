FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.5e-4;  

boundaryField
{
    
    "(inlet|farfield_inlet)"
    {
        type            fixedValue;
        value           $internalField;
    }

    "(outlet|farfield_outlet)"
    {
        type            zeroGradient;
    }

    "(ubend_inner|ubend_outer|farfield_top|farfield_right|farfield_left|farfield_bottom)"
    {
        type            fixedValue;
        value           uniform 0;
    }

}


